{
  "expo": {
    "name": "Aidas Corners",
    "slug": "aidas-corners",
    "version": "1.3.0a",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Excel dosyalarını kaydetmek ve paylaşmak için izin gerekli",
        "NSDocumentsFolderUsageDescription": "Excel dosyalarını kaydetmek ve paylaşmak için izin gerekli",
        "LSSupportsOpeningDocumentsInPlace": true,
        "UIFileSharingEnabled": true
      },
      "bundleIdentifier": "com.ihsynzd.aidascorners"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "translucent": true
      },
      "package": "com.ihsynzd.aidascorners",
      "permissions": [
        "QUERY_ALL_PACKAGES",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "MEDIA_LIBRARY",
        "DOWNLOADS",
        "POST_NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "USE_EXACT_ALARM",
        "VIBRATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "whatsapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#000000",
      "hidden": true,
      "translucent": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-file-system",
        {
          "filePermission": "Uygulama dosya erişimi için gerekli"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6f3777e0-d5c9-434b-b7cf-ff0b1c7a127b"
      }
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/6f3777e0-d5c9-434b-b7cf-ff0b1c7a127b"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "channel": "production"
  }
}